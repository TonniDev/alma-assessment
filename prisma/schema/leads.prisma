enum EVisa {
  O1        @map("O-1")
  EB1A      @map("EB-1A")
  EB2NIW    @map("EB-2-NIW")
  NOT_KNOWN @map("not_known")
}

enum EStatus {
  PENDING
  REACHED_OUT
}

model leads {
  id                   String   @unique @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  firstName            String   @map(name: "first_name") @db.VarChar(255)
  lastName             String   @map(name: "last_name") @db.VarChar(255)
  email                String   @map(name: "email") @db.VarChar(255)
  countryOfCitizenship ECountry @map(name: "country_of_citizenship")
  linkedin             String   @db.VarChar(255)
  visaInterest         EVisa[]  @map(name: "visa_interest")
  description          String   @db.Text
  status               EStatus
  resumeUrl            String   @db.VarChar(255)
  created              DateTime @default(now())
  modified             DateTime @updatedAt

  @@index([firstName, email])
}
